{
  "name": "politico-react",
  "version": "1.0.0",
  "description": "An application for voting into political office",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text-lcov  echo \"Error: no test specified\" && exit 0",
    "coverage": "nyc report --reporter=text-lcov || coveralls",
    "dev:start": "rm -rf dist && webpack-dev-server --env.mode development --open --hot",
    "webpack": "webpack --env.mode development",
    "client:build": "webpack --env.mode production",
    "heroku:postbuild": "npm run client:build",
    "start": "npm run heroku:postbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantastic-genius/politico-react.git"
  },
  "author": "Hamzah Abdulfattah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fantastic-genius/politico-react/issues"
  },
  "homepage": "https://github.com/fantastic-genius/politico-react#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ext .json --ext .js --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
